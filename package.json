{
  "name": "@nathanfaucett/virt-dom",
  "version": "0.0.15",
  "description": "virt DOM is a render adapter for virt",
  "repository": {
    "type": "git",
    "url": "https://github.com/nathanfaucett/virt-dom.git"
  },
  "main": "src/index.js",
  "keywords": [
    "virt",
    "dom",
    "adapter",
    "virt-dom",
    "virt-dom.js"
  ],
  "scripts": {
    "input_events": "comn -f ./examples/input_events/src/index.js -o ./examples/input_events/index.min.js",
    "simple": "comn -f ./examples/simple/src/index.js -o ./examples/simple/index.min.js",
    "replace": "comn -f ./examples/replace/src/index.js -o ./examples/replace/index.min.js",
    "components": "comn -f ./examples/components/src/index.js -o ./examples/components/index.min.js",
    "workers": "comn -f ./examples/workers/src/index.js -o ./examples/workers/index.min.js && comn -f ./examples/workers/src/worker.js -o ./examples/workers/worker.min.js",
    "worker_form": "comn -f ./examples/worker_form/src/index.js -o ./examples/worker_form/index.min.js && comn -f ./examples/worker_form/src/worker.js -o examples/worker_form/worker.min.js",
    "websocket_form": "comn -f ./examples/websocket_form/src/index.js -o ./examples/websocket_form/index.min.js",
    "examples": "npm run input_events && npm run simple && npm run replace && npm run components && npm run workers && npm run worker_form && npm run websocket_form",
    "build": "task && task dist && npm run examples"
  },
  "author": "Nathan Faucett",
  "license": "MIT",
  "readmeFilename": "README.md",
  "devDependencies": {
